      }
    },
    "Uno": {
      "title": "Uno",
      "playTime": "15min",
      "description": "Uno is a classic card game of matching colors and numbers. It's easy to pick up but hard to put down! Play special action cards to change the game's direction.",
      "howToPlay": {
        "0": "Each player is dealt 7 cards.",
        "1": "Match the top card on the discard pile by number, color, or action.",
        "2": "If you can't play, draw a card.",
        "3": "Use Action cards (Skip, Reverse, Draw Two, Wild, Wild Draw Four) strategically.",
        "4": "Shout 'UNO!' when you have one card left."
      }
    },
    "Chess": {
      "title": "Chess",
      "playTime": "25min",
      "description": "Chess is a strategy board game for two players. The objective is to checkmate the opponent's king, putting it under an inescapable threat of capture.",
      "howToPlay": {
        "0": "Each piece moves in a specific way (Pawn, Rook, Knight, Bishop, Queen, King).",
        "1": "White moves first, then players alternate turns.",
        "2": "Capture opponent's pieces by landing on their square.",
        "3": "The game ends when a king is in checkmate (cannot escape capture).",
        "4": "Learn special moves like Castling and En Passant."
      }
    },
    "HangMan": {
      "title": "HangMan",
      "playTime": "10min",
      "description": "Hangman is a fun word-guessing game. One player thinks of a word, and the other tries to guess it by suggesting letters within a limited number of attempts.",
      "howToPlay": {
        "0": "A secret word is chosen, represented by blank spaces.",
        "1": "Guess letters you think are in the word.",
        "2": "Correct guesses reveal the letter's position(s).",
        "3": "Incorrect guesses add a part to the hangman drawing.",
        "4": "Guess the word before the hangman is fully drawn to win!"
      }
    }
